[user]
	name = Jason Holloway
	email = jason.holloway@laterooms.com 
[core]
	autocrlf = input
[push]
	default = matching
[alias]
	lg = log --oneline --decorate --graph
	br = branch
	st = status
	cm = commit -m
	co = checkout
	conflicts = diff --name-only --diff-filter=U
	makeexec = !sh -c 'if [[ ${0: -3} == ".sh" ]]; then git update-index --chmod=+x $0; git add $0'
[push]
	default = current

