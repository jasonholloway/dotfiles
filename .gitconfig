[core]
	fscache = true
	autocrlf = false
[credential]
	helper = manager
[alias]
	st = status
	co = checkout
	cp = cherry-pick
	br = branch
	cm = commit -m
	lg = log --graph --oneline
	fa = fetch --all
	pf = push --force
	rc = rebase --continue
	ra = rebase --abort
	rs = rebase --skip
	ri = rebase -i
	rom = rebase origin/master
	po = push origin
	pof = push origin -f
	conflicts = diff --name-only --diff-filter=U
	makeexec = !sh -c 'if [[ ${0: -3} == ".sh" ]]; then git update-index --chmod=+x $0; git add $0'
[user]
	name = Jason Holloway
	email = jason.holloway@sorted.com
[push]
	default = current
[pull]
	ff = only
[diff]
	tool = vimdiff
